{
  "name": "sons-of-the-forest-map",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite -c vite-overwolf.config.ts",
    "dev:web": "vite -c vite-web.config.ts",
    "build": "tsc && vite build  -c vite-overwolf.config.ts && node copy-statics.mjs && ow opk pack dist -o soft.opk && ow opk sign soft.opk",
    "web": "tsc && vite build -c vite-web.config.ts",
    "preview": "vite preview",
    "release": "ow opk upload soft.signed.opk"
  },
  "devDependencies": {
    "@overwolf/ow-cli": "0.0.5-0",
    "@overwolf/types": "3.23.1",
    "@types/leaflet": "1.9.1",
    "@types/qrcode": "1.5.0",
    "@types/throttle-debounce": "5.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-pwa": "0.14.7"
  },
  "dependencies": {
    "@geoman-io/leaflet-geoman-free": "2.14.2",
    "leaflet": "1.9.3",
    "peerjs": "1.4.7",
    "qrcode": "1.5.1",
    "throttle-debounce": "5.0.0"
  }
}
